{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@routes/*": ["src/routes/*"],
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/icons/*"],
      "@constants/*": ["src/constants/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}
